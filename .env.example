# Database Configuration
DATABASE_URL=postgresql://eitaa_user:eitaa_pass@localhost:5432/eitaa

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# OpenAI API Key (for embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Rate Limiting
RATE_PER_SECOND=2

# Session Configuration (comma-separated phone numbers)
SESSIONS=acc1,acc2,acc3

# Eitaa API Configuration
# توکن از https://eitaayar.ir (برای ارسال پیام؛ خواندن کانال بدون توکن هم کار می‌کند)
EITAA_TOKEN=your_eitaa_token_here
# لیست آیدی کانال‌های واقعی (با کاما). اگر پر باشد، داده از همون کانال‌ها از API ایتا گرفته می‌شود
# مثال: eitaa_faq,eitaa
EITAA_CHANNEL_IDS=channel_ids_here
